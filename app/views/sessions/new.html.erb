<div class= main>
  <h3>ログイン</h3>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <br>
    <table class="user_form">
      <tr>
        <th><%= f.label :メールアドレス %></th>
        <td><%= f.email_field :email %></td>
      </tr>
      <tr>
        <th><%= f.label :パスワード %></th>
        <td><%= f.password_field :password %></td>
      </tr>
    </table>
    <br>
    <%= f.submit "ログイン" %>
  <% end %>
</div>
